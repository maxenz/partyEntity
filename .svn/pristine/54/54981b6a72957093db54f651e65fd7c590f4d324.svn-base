Imports WeifenLuo.WinFormsUI.Docking
Public Class frmExportaciones

    Private Sub frmExportaciones_Load(ByVal sender As Object, ByVal e As System.EventArgs) Handles Me.Load
        Me.FormatScreen()
    End Sub

    Private Sub FormatScreen()
        '----> Formulario
        Me.BackColor = Color.Beige
        '----> Paquetes
        Me.cmbArchivos.Properties.Items.Add(New ShamanClases.ItemCombo(1, "Sintomas"))
        Me.cmbArchivos.SelectedIndex = 0
    End Sub

    Private Sub btnGenerador_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnGenerador.Click
        Dim objSintomas As New ShamanClases.EmergencyC.Sintomas

        objSintomas.Exportar()

        MsgBox("Terminó la exportación de " & Me.cmbArchivos.Text, MsgBoxStyle.Information, "Exportaciones")

        objSintomas = Nothing

    End Sub
End Class