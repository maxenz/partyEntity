Imports WeifenLuo.WinFormsUI.Docking
Public Class frmTrasladosSpace
    Inherits DockContent

    Private flgLoading As Boolean = True

    Private Sub frmTrasladosSpace_GotFocus(ByVal sender As Object, ByVal e As System.EventArgs) Handles Me.GotFocus

        If Not flgLoading Then
            If Val(callInfo) > 0 Then
                ShowIncidenteScreen(Val(callInfo))
            Else
                ManagePages(ennPages.emeTraslados)
            End If
        Else
            flgLoading = False
        End If

    End Sub
    Private Sub frmTrasladosSpace_Load(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles MyBase.Load

        Me.dockTraslados.Parent = Me
        Me.dockTraslados.DocumentStyle = WeifenLuo.WinFormsUI.Docking.DocumentStyle.DockingSdi

    End Sub

End Class