Imports WeifenLuo.WinFormsUI.Docking
Public Class frmGeneradorClases

    Private Sub frmGeneradorClases_Load(ByVal sender As Object, ByVal e As System.EventArgs) Handles Me.Load
        Me.FormatScreen()
    End Sub

    Private Sub FormatScreen()
        '----> Formulario
        Me.BackColor = Color.Beige
        '----> Paquetes
        Me.cmbPaquetes.Properties.Items.Add(New ShamanClases.ItemCombo(1, "Emergency"))
        Me.cmbPaquetes.Properties.Items.Add(New ShamanClases.ItemCombo(2, "Ventas"))
        Me.cmbPaquetes.Properties.Items.Add(New ShamanClases.ItemCombo(3, "Rrhh"))
        Me.cmbPaquetes.Properties.Items.Add(New ShamanClases.ItemCombo(4, "CompuMap"))
        Me.cmbPaquetes.Properties.Items.Add(New ShamanClases.ItemCombo(5, "Panel"))
        Me.cmbPaquetes.Properties.Items.Add(New ShamanClases.ItemCombo(6, "Transito"))
        Me.cmbPaquetes.SelectedIndex = 0
    End Sub

    Private Sub btnGenerador_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnGenerador.Click
        Dim objGenerador As New ShamanClases.PanelC.GeneradorClases

        objGenerador.Generar(Me.pgbProceso, Me.lblProceso, Me.cmbPaquetes.Text)

        MsgBox("Terminó el proceso de Generación de clases del paquete " & Me.cmbPaquetes.Text, MsgBoxStyle.Information, "Generador")

        objGenerador = Nothing

    End Sub
End Class